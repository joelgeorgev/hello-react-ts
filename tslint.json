{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "eofline": false,
    "indent": [
      true,
      "spaces",
      2
    ],
    "ordered-imports": [
      true,
      {
        "import-sources-order": "any"
      }
    ]
  },
  "rulesDirectory": []
}