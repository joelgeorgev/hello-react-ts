{
  "name": "react-ts-setup-guide",
  "version": "1.0.0",
  "description": "Step by step guide to setup a React app written in TypeScript.",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "lint": "tslint -c tslint.json './src/**/*.ts*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelgeorgev/react-ts-setup-guide.git"
  },
  "author": "Joel George V",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joelgeorgev/react-ts-setup-guide/issues"
  },
  "homepage": "https://joelgeorgev.github.io/react-ts-setup-guide",
  "devDependencies": {
    "@types/enzyme": "^3.1.9",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.1.2",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "clean-webpack-plugin": "^0.1.18",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.2.1",
    "path": "^0.12.7",
    "ts-jest": "^22.0.3",
    "ts-loader": "^3.4.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.6.0",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "React",
    "reactjs",
    "TypeScript",
    "ts",
    "webpack"
  ]
}
