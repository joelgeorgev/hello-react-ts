{
  "name": "react-ts-setup-guide",
  "version": "1.0.0",
  "description": "Step by step guide to setup a React app written in TypeScript.",
  "main": "index.tsx",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "lint": "tslint -c tslint.json './src/**/*.ts*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joelgeorgev/react-ts-setup-guide.git"
  },
  "author": "Joel George V",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joelgeorgev/react-ts-setup-guide/issues"
  },
  "homepage": "https://joelgeorgev.github.io/react-ts-setup-guide",
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "clean-webpack-plugin": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "gh-pages": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.0.1",
    "path": "^0.12.7",
    "ts-jest": "^26.1.0",
    "ts-loader": "^9.4.2",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "typescript": "^3.9.5",
    "webpack": "^5.75.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "React",
    "reactjs",
    "TypeScript",
    "ts",
    "webpack"
  ]
}
